{
  "name": "next-dashboard-rambo",
  "description": "Dashboard for statuses",
  "version": "2.0.24",
  "license": "MIT",
  "private": true,
  "author": {
    "name": "Geir GÃ¥sodden",
    "email": "geir.gasodden@pythonia.no",
    "url": "https://github.com/zrrrzzt"
  },
  "engines": {
    "node": ">=7.10.0"
  },
  "scripts": {
    "test": "standard && nsp check && ava",
    "test-offline": "standard && ava",
    "coverage": "nyc ava",
    "coveralls": "nyc ava && nyc report --reporter=lcov && cat coverage/lcov.info | coveralls",
    "standard-fix": "standard --fix",
    "dev": "next",
    "build": "next build && next export",
    "start": "next start",
    "now-deploy": "npm test && npm run build && now out --static -n dashboard-rambo && now alias"
  },
  "keywords": [
    "dashboard",
    "next",
    "status",
    "rambo",
    "Telemark fylkeskommune"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/telemark/next-dashboard-rambo.git"
  },
  "bugs": {
    "url": "https://github.com/telemark/next-dashboard-rambo/issues"
  },
  "homepage": "https://github.com/telemark/next-dashboard-rambo",
  "now": {
    "name": "dashboard-rambo",
    "alias": [
      "rambo.dashboard.tjeneste.win"
    ],
    "env": {
      "NODE_ENV": "production"
    },
    "type": "npm"
  },
  "devDependencies": {
    "ava": "0.22.0",
    "coveralls": "2.13.1",
    "nsp": "2.8.0",
    "nyc": "11.2.1",
    "standard": "10.0.3"
  },
  "dependencies": {
    "axios": "0.16.2",
    "muicss": "0.9.24",
    "next": "3.2.0",
    "react": "15.5.4",
    "react-dom": "15.5.4"
  }
}