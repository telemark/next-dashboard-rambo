{
  "name": "next-dashboard-rambo",
  "description": "Dashboard for statuses",
  "version": "3.0.4",
  "license": "MIT",
  "private": true,
  "author": {
    "name": "Geir GÃ¥sodden",
    "email": "geir.gasodden@pythonia.no",
    "url": "https://github.com/zrrrzzt"
  },
  "engines": {
    "node": ">=8.9.4"
  },
  "scripts": {
    "test": "standard && nsp check && ava",
    "test-offline": "standard && ava",
    "coverage": "nyc ava",
    "coveralls": "nyc ava && nyc report --reporter=lcov && cat coverage/lcov.info | coveralls",
    "standard-fix": "standard --fix",
    "dev": "next",
    "build": "next build",
    "export": "next build && next export",
    "start": "next start",
    "deploy": "npm test && npm run export && now out --static -n dashboard-rambo && now alias",
    "refresh": "rm -rf node_modules && rm package-lock.json && npm install"
  },
  "keywords": [
    "dashboard",
    "next",
    "status",
    "rambo",
    "Telemark fylkeskommune"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/telemark/next-dashboard-rambo.git"
  },
  "bugs": {
    "url": "https://github.com/telemark/next-dashboard-rambo/issues"
  },
  "homepage": "https://github.com/telemark/next-dashboard-rambo#readme",
  "now": {
    "name": "dashboard-rambo",
    "alias": [
      "rambo.dashboard.next.t-fk.no"
    ],
    "env": {
      "NODE_ENV": "production"
    },
    "type": "npm"
  },
  "devDependencies": {
    "ava": "0.25.0",
    "coveralls": "3.0.1",
    "nsp": "3.2.1",
    "nyc": "12.0.2",
    "standard": "11.0.1"
  },
  "dependencies": {
    "axios": "0.18.0",
    "next": "6.1.1",
    "react": "16.4.1",
    "react-dom": "16.4.1"
  }
}
